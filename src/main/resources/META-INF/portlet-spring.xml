<?xml version="1.0"?>

<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<bean class="com.example.inventory.service.impl.ProductLocalServiceImpl" id="com.example.inventory.service.ProductLocalService" />
	<bean class="com.example.inventory.service.impl.ProductServiceImpl" id="com.example.inventory.service.ProductService" />
	<bean class="com.example.inventory.service.persistence.impl.ProductModelArgumentsResolver" id="com.example.inventory.service.persistence.impl.ProductModelArgumentsResolver" />
	<bean class="com.example.inventory.service.persistence.impl.ProductPersistenceImpl" id="com.example.inventory.service.persistence.ProductPersistence" />
</beans>